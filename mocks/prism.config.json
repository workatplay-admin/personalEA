{
  "mock": {
    "dynamic": true,
    "cors": true,
    "validateRequest": true,
    "validateResponse": true,
    "checkSecurity": true,
    "errors": false
  },
  "server": {
    "host": "0.0.0.0",
    "port": 4010
  },
  "logging": {
    "level": "info",
    "prettyPrint": true
  },
  "examples": {
    "preference": "dynamic",
    "fallback": "generated"
  },
  "responses": {
    "headers": {
      "X-Powered-By": "PersonalEA Mock Server",
      "X-Mock-Version": "1.0.0"
    }
  },
  "delays": {
    "enabled": true,
    "min": 100,
    "max": 500
  },
  "stateful": {
    "enabled": true,
    "storage": "memory"
  }
}